{
  "name": "vimflowy",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "0.10.x",
    "npm": "1.2.x"
  },
  "scripts": {
    "start": "NODE_ENV=development nodemon -e coffee,jade,sass server.coffee",
    "test": "multi='spec=- html-cov=public/coverage.html' node_modules/mocha/bin/mocha --require blanket --reporter mocha-multi --compilers coffee:coffee-script/register tests/tests/*"
  },
  "config": {
    "blanket": {
      "pattern": "/\/assets\/js\/.*\\.coffee$/",
      "loader": "./node-loaders/coffee-script"
    }
  },
  "devDependencies": {
    "blanket": "latest",
    "mocha": "latest",
    "mocha-multi": "latest",
    "nodemon": "latest"
  },
  "dependencies": {
    "async": "latest",
    "coffee-script": "latest",
    "connect-assets": "latest",
    "connect-flash": "latest",
    "express": "latest",
    "jade": "latest",
    "lodash": "latest",
    "node-sass": "latest"
  }
}
